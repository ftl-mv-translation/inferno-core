#: src-en/data/events_special_lightspeed.xml.append:3
msgid ""
"data/events_special_lightspeed.xml.append$//event[@name=\"FMCORE_ATLAS_EQUIPMENT\"]/text"
msgstr ""
"Wähle aus, was du an Bord des Schiffes während des Hypersprungs machen "
"möchtest. Du hast mehre Optionen."

#: src-en/data/events_special_lightspeed.xml.append:5
msgid ""
"data/events_special_lightspeed.xml.append$//event[@name=\"FMCORE_ATLAS_EQUIPMENT\"]/choice[@req=\"FMCORE_CAN_USE_HS_AUG\"]/text"
msgstr "Nutze ein Sprunggeschwindigkeits-Ausbau."

#: src-en/data/events_special_lightspeed.xml.append:12
msgid ""
"data/events_special_lightspeed.xml.append$//event[@name=\"FMCORE_ATLAS_EQUIPMENT\"]/choice[@req=\"fmcore_used_hs_aug\"]/text"
msgstr "Du hast bereits einen Sprunggeschwindigkeits-Ausbau verwendet."

#: src-en/data/events_special_lightspeed.xml.append:16
msgid ""
"data/events_special_lightspeed.xml.append$//event[@name=\"FMCORE_ATLAS_EQUIPMENT\"]/choice[@req=\"FMCORE_CAN_CALL_CURA\"]/text"
msgstr "Verfügbare Aufgaben von C.U.R.A. anzeigen."

#: src-en/data/events_special_lightspeed.xml.append:26
msgid ""
"data/events_special_lightspeed.xml.append$//event[@name=\"FMCORE_ATLAS_EQUIPMENT\"]/choice[@req=\"fmcore_called_cura\"]/text"
msgstr "DU hast bereits eine C.U.R.A Aufgabe angenommen."

#: src-en/data/events_special_lightspeed.xml.append:30
msgid ""
"data/events_special_lightspeed.xml.append$//event[@name=\"FMCORE_ATLAS_EQUIPMENT\"]/choice[@req=\"pilot\"]/text"
msgstr "Doch nicht."

#: src-en/data/events_special_lightspeed.xml.append:42
msgid ""
"data/events_special_lightspeed.xml.append$//mod:findName[@name=\"ATLAS_MENU_NOEQUIPMENT\"]/mod:findWithChildLike/mod-"
"overwrite:text"
msgstr "Tue etwas anderes, an Bord des Schiffes."

#: src-en/data/events_special_lightspeed.xml.append:45
msgid ""
"data/events_special_lightspeed.xml.append$//mod:findName[@name=\"ATLAS_MENU_NOEQUIPMENT\"]/mod-"
"append:choice/text"
msgstr "Zur Föderationshauptstadt weiterreisen."
